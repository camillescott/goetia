PATH_OPERATIONS=$( dirname "${BASH_SOURCE[0]}" )


#==================================================================================================
# Dense vector/dense vector addition
#==================================================================================================

$PATH_OPERATIONS/dvecdvecadd/run; if [ $? != 0 ]; then exit 1; fi


